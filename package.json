{
  "name": "iis-express-vscode",
  "displayName": "IIS Express for VSCode",
  "description": "IIS Express integration for VSCode",
  "icon": "images/logo.png",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "IIS",
    "IIS Express",
    "Web developpement",
    "C#",
    "dotnet",
    ".NET",
    "ASP.NET"
  ],
  "packageManager": "yarn@1.22.21",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "iis-express.initialize",
        "title": "IIS Express: Initialize"
      },
      {
        "command": "iis-express.stop",
        "title": "IIS Express: Stop"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/ps-node": "^0.1.3",
    "@types/uuid": "^9.0.8",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "fast-xml-parser": "^4.4.0",
    "json5": "^2.2.3",
    "uuid": "^9.0.1"
  }
}